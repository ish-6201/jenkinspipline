pipeline {

agent any

stages {
	stage('SCM'){
		steps {
			echo "git pull my code step1"
			echo "git pull my code step2"
              	}
	}
	stage('Deploy') {
		steps {
			echo "Deploying pull my code"
         	      }
	}

	stage('Test'){
		steps {
			echo "Test my final code"
               	}
      	}
	}
	stage('Deploy To Production'){
			steps {
				echo "my final webapp to prod"
               		}
      	}
	}

} 